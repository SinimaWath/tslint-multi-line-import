{
  "name": "multiline-import",
  "version": "0.1.5",
  "description": "TSLint rules to check multiline imports",
  "main": "dist/multiLineImportRule.js",
  "homepage": "https://github.com/SinimaWath/tslint-multi-line-import",
  "repository": {
    "type": "git",
    "url": "https://github.com/SinimaWath/tslint-multi-line-import"
  },
  "scripts": {
    "build": "tsc --p tsconfig.json",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "tslint",
    "multiline-import",
    "import",
    "rules"
  ],
  "author": {
    "name": "Vladislav Tarasov",
    "email": "sinimawath@gmail.com"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.8",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "tslint": "^5.17.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/**/*.test.(ts|tsx)"
    ],
    "testEnvironment": "node"
  }
}
